nmodes 32;
txvcosplit 150.000;
rxvcosplit 154.000;

# Power: mode 1=high; mode 2=low

mode 1
{
	txfreq 150.000;
	rxfreq 150.000;
	txpower high;
};
mode 2
{
	txfreq 150.000;
	rxfreq 150.000;
	txpower low;
};
